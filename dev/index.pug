doctype
html
  include chunks/variables 
  head
    include chunks/head
  body
    include chunks/header
    .wrapper
      .row.clearfix
        each item,index in articleTitle
          .col-tablet-1-2.col-desktop-1-3
            a(href="#").article(class=index % 3 - 2 ==0 ? 'article-l-orange' : index % 3 - 1 == 0 ? 'article-m-orange' : '')
              include chunks/article
        each item,index in catalogTitle
          .col-tablet-1-3.col-desktop-1-4
            a(href="#").article.article-gray
              include chunks/article
